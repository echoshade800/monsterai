{
  "expo": {
    "name": "MonsterAI",
    "slug": "MonsterAI",
    "version": "1.0.0",
    "scheme": "monsterai",
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.fanthus.monsterai.debug",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"]
      }
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-build-properties",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Monster AI to access your camera to record moments."
        }
      ],
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.827695368889-p02agvs6fcqqj0djmumg5ddldjdpjlmh"
        }
      ]
    ],
    "android": {
      "package": "com.fanthus.monsterai.debug"
    },
    "extra": {
      "eas": {
        "projectId": "8379ece1-4306-4d0e-9c26-cdbb5c8efd41"
      }
    },
    "owner": "fanthus"
  }
}
